<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- ============================================
         DASHBOARD CLIENT ACTION
         ============================================ -->
    <record id="action_university_dashboard" model="ir.actions.client">
        <field name="name">University Dashboard</field>
        <field name="tag">university_dashboard</field>
        <field name="target">current</field>
    </record>


    <!-- ============================================
         DASHBOARD CONFIGURATION VIEWS
         ============================================ -->

    <!-- Dashboard List View -->
    <record id="view_university_dashboard_list" model="ir.ui.view">
        <field name="name">university.dashboard.list</field>
        <field name="model">university.dashboard</field>
        <field name="arch" type="xml">
            <list string="Dashboards" default_order="sequence">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
                <field name="user_type"/>
                <field name="layout"/>
                <field name="is_default" widget="boolean_toggle"/>
                <field name="active" widget="boolean_toggle"/>
            </list>
        </field>
    </record>

    <!-- Dashboard Form View -->
    <record id="view_university_dashboard_form" model="ir.ui.view">
        <field name="name">university.dashboard.form</field>
        <field name="model">university.dashboard</field>
        <field name="arch" type="xml">
            <form string="Dashboard">
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object"
                                class="oe_stat_button" icon="fa-archive">
                            <field name="active" widget="boolean_button"
                                   options='{"terminology": "archive"}'/>
                        </button>
                    </div>

                    <widget name="web_ribbon" title="Default" bg_color="bg-success"
                            invisible="not is_default"/>

                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="sequence"/>
                        </group>
                        <group>
                            <field name="user_type"/>
                            <field name="layout"/>
                            <field name="is_default"/>
                            <field name="allow_customization"/>
                        </group>
                    </group>

                    <group>
                        <field name="description" placeholder="Dashboard description..."/>
                    </group>

                    <notebook>
                        <page string="Widgets" name="widgets">
                            <field name="widget_ids">
                                <list editable="bottom" default_order="sequence">
                                    <field name="sequence" widget="handle"/>
                                    <field name="name"/>
                                    <field name="code"/>
                                    <field name="widget_type"/>
                                    <field name="width"/>
                                    <field name="height"/>
                                    <field name="active" widget="boolean_toggle"/>
                                </list>
                            </field>
                        </page>

                        <page string="Access Control" name="access">
                            <group>
                                <field name="group_ids" widget="many2many_tags"/>
                            </group>
                        </page>

                        <page string="Settings" name="settings">
                            <group>
                                <group>
                                    <field name="refresh_interval"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Dashboard Search View -->
    <record id="view_university_dashboard_search" model="ir.ui.view">
        <field name="name">university.dashboard.search</field>
        <field name="model">university.dashboard</field>
        <field name="arch" type="xml">
            <search string="Search Dashboards">
                <field name="name"/>
                <field name="code"/>
                <field name="user_type"/>
                <separator/>
                <filter string="Default" name="is_default" domain="[('is_default', '=', True)]"/>
                <filter string="Active" name="active" domain="[('active', '=', True)]"/>
                <filter string="Archived" name="archived" domain="[('active', '=', False)]"/>
                <group expand="0" string="Group By">
                    <filter string="User Type" name="group_user_type" context="{'group_by': 'user_type'}"/>
                    <filter string="Layout" name="group_layout" context="{'group_by': 'layout'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Dashboard Action -->
    <record id="action_university_dashboard_config" model="ir.actions.act_window">
        <field name="name">Dashboard Configuration</field>
        <field name="res_model">university.dashboard</field>
        <field name="view_mode">list,form</field>
        <field name="context">{'search_default_active': 1}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first dashboard configuration
            </p>
            <p>
                Dashboards provide a comprehensive overview of university data.
            </p>
        </field>
    </record>


    <!-- ============================================
         DASHBOARD WIDGET VIEWS
         ============================================ -->

    <!-- Widget List View -->
    <record id="view_dashboard_widget_list" model="ir.ui.view">
        <field name="name">university.dashboard.widget.list</field>
        <field name="model">university.dashboard.widget</field>
        <field name="arch" type="xml">
            <list string="Dashboard Widgets" default_order="sequence">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
                <field name="dashboard_id"/>
                <field name="widget_type"/>
                <field name="width"/>
                <field name="active" widget="boolean_toggle"/>
            </list>
        </field>
    </record>

    <!-- Widget Form View -->
    <record id="view_dashboard_widget_form" model="ir.ui.view">
        <field name="name">university.dashboard.widget.form</field>
        <field name="model">university.dashboard.widget</field>
        <field name="arch" type="xml">
            <form string="Dashboard Widget">
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object"
                                class="oe_stat_button" icon="fa-archive">
                            <field name="active" widget="boolean_button"
                                   options='{"terminology": "archive"}'/>
                        </button>
                    </div>

                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="dashboard_id"/>
                            <field name="sequence"/>
                        </group>
                        <group>
                            <field name="widget_type"/>
                            <field name="width"/>
                            <field name="height"/>
                            <field name="icon"/>
                            <field name="color" widget="color"/>
                        </group>
                    </group>

                    <notebook>
                        <page string="Data Source" name="data_source">
                            <group>
                                <group>
                                    <field name="model_name"/>
                                    <field name="data_method"/>
                                </group>
                                <group>
                                    <field name="domain" widget="domain"
                                           options="{'model': 'model_name'}"/>
                                </group>
                            </group>
                        </page>

                        <page string="Display" name="display">
                            <group>
                                <field name="background_color" widget="color"/>
                                <field name="config" widget="ace" options="{'mode': 'json'}"/>
                            </group>
                        </page>

                        <page string="Access Control" name="access">
                            <field name="group_ids" widget="many2many_tags"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Widget Search View -->
    <record id="view_dashboard_widget_search" model="ir.ui.view">
        <field name="name">university.dashboard.widget.search</field>
        <field name="model">university.dashboard.widget</field>
        <field name="arch" type="xml">
            <search string="Search Widgets">
                <field name="name"/>
                <field name="code"/>
                <field name="dashboard_id"/>
                <field name="widget_type"/>
                <separator/>
                <filter string="Active" name="active" domain="[('active', '=', True)]"/>
                <group expand="0" string="Group By">
                    <filter string="Dashboard" name="group_dashboard" context="{'group_by': 'dashboard_id'}"/>
                    <filter string="Widget Type" name="group_type" context="{'group_by': 'widget_type'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Widget Action -->
    <record id="action_dashboard_widget" model="ir.actions.act_window">
        <field name="name">Dashboard Widgets</field>
        <field name="res_model">university.dashboard.widget</field>
        <field name="view_mode">list,form</field>
        <field name="context">{'search_default_active': 1}</field>
    </record>


    <!-- ============================================
         DASHBOARD KPI VIEWS
         ============================================ -->

    <!-- KPI List View -->
    <record id="view_dashboard_kpi_list" model="ir.ui.view">
        <field name="name">university.dashboard.kpi.list</field>
        <field name="model">university.dashboard.kpi</field>
        <field name="arch" type="xml">
            <list string="Dashboard KPIs" default_order="sequence">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
                <field name="category"/>
                <field name="last_value" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                <field name="target_value"/>
                <field name="last_calculated" widget="datetime"/>
                <field name="active" widget="boolean_toggle"/>
                <field name="currency_id" invisible="1"/>
            </list>
        </field>
    </record>

    <!-- KPI Form View -->
    <record id="view_dashboard_kpi_form" model="ir.ui.view">
        <field name="name">university.dashboard.kpi.form</field>
        <field name="model">university.dashboard.kpi</field>
        <field name="arch" type="xml">
            <form string="Dashboard KPI">
                <header>
                    <button name="calculate_kpi" type="object" string="Calculate Now"
                            class="btn-primary"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object"
                                class="oe_stat_button" icon="fa-archive">
                            <field name="active" widget="boolean_button"
                                   options='{"terminology": "archive"}'/>
                        </button>

                        <button name="calculate_kpi" type="object" class="oe_stat_button"
                                icon="fa-calculator">
                            <div class="o_field_widget o_stat_info">
                                <span class="o_stat_value">
                                    <field name="last_value" widget="statinfo"/>
                                </span>
                                <span class="o_stat_text">Current Value</span>
                            </div>
                        </button>
                    </div>

                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="category"/>
                            <field name="sequence"/>
                        </group>
                        <group>
                            <field name="icon"/>
                            <field name="color" widget="color"/>
                            <field name="unit"/>
                            <field name="prefix"/>
                            <field name="suffix"/>
                        </group>
                    </group>

                    <group string="Description">
                        <field name="description" nolabel="1" placeholder="KPI description..."/>
                    </group>

                    <notebook>
                        <page string="Calculation" name="calculation">
                            <group>
                                <group>
                                    <field name="model_name"/>
                                    <field name="field_name"/>
                                    <field name="aggregation"/>
                                </group>
                                <group>
                                    <field name="domain" widget="domain"
                                           options="{'model': 'model_name'}"/>
                                </group>
                            </group>
                        </page>

                        <page string="Targets &amp; Thresholds" name="targets">
                            <group>
                                <group>
                                    <field name="target_value"/>
                                    <field name="warning_threshold"/>
                                    <field name="critical_threshold"/>
                                </group>
                                <group>
                                    <field name="last_calculated" readonly="1"/>
                                    <field name="last_value" readonly="1"/>
                                    <field name="cache_duration"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- KPI Kanban View -->
    <record id="view_dashboard_kpi_kanban" model="ir.ui.view">
        <field name="name">university.dashboard.kpi.kanban</field>
        <field name="model">university.dashboard.kpi</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile">
                <field name="name"/>
                <field name="code"/>
                <field name="last_value"/>
                <field name="target_value"/>
                <field name="unit"/>
                <templates>
                    <t t-name="kanban-box">
                        <div class="oe_kanban_global_click">
                            <div class="row">
                                <div class="col-12">
                                    <strong><field name="name"/></strong>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-8">
                                    <span class="text-muted">Code: </span>
                                    <field name="code"/>
                                </div>
                                <div class="col-4 text-right">
                                    <span class="badge badge-info">
                                        <field name="category"/>
                                    </span>
                                </div>
                            </div>
                            <div class="row mt-2">
                                <div class="col-6">
                                    <span class="text-muted">Value: </span>
                                    <strong><field name="last_value"/> <field name="unit"/></strong>
                                </div>
                                <div class="col-6">
                                    <span class="text-muted">Target: </span>
                                    <field name="target_value"/> <field name="unit"/>
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <!-- KPI Search View -->
    <record id="view_dashboard_kpi_search" model="ir.ui.view">
        <field name="name">university.dashboard.kpi.search</field>
        <field name="model">university.dashboard.kpi</field>
        <field name="arch" type="xml">
            <search string="Search KPIs">
                <field name="name"/>
                <field name="code"/>
                <field name="category"/>
                <separator/>
                <filter string="Active" name="active" domain="[('active', '=', True)]"/>
                <group expand="0" string="Group By">
                    <filter string="Category" name="group_category" context="{'group_by': 'category'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- KPI Action -->
    <record id="action_dashboard_kpi" model="ir.actions.act_window">
        <field name="name">Dashboard KPIs</field>
        <field name="res_model">university.dashboard.kpi</field>
        <field name="view_mode">kanban,list,form</field>
        <field name="context">{'search_default_active': 1}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first KPI
            </p>
            <p>
                KPIs help track key metrics and performance indicators.
            </p>
        </field>
    </record>


    <!-- ============================================
         DASHBOARD USER PREFERENCE VIEWS
         ============================================ -->

    <!-- Preference List View -->
    <record id="view_dashboard_preference_list" model="ir.ui.view">
        <field name="name">university.dashboard.preference.list</field>
        <field name="model">university.dashboard.preference</field>
        <field name="arch" type="xml">
            <list string="Dashboard Preferences">
                <field name="user_id"/>
                <field name="dashboard_id"/>
                <field name="theme"/>
                <field name="date_range"/>
                <field name="refresh_interval"/>
            </list>
        </field>
    </record>

    <!-- Preference Form View -->
    <record id="view_dashboard_preference_form" model="ir.ui.view">
        <field name="name">university.dashboard.preference.form</field>
        <field name="model">university.dashboard.preference</field>
        <field name="arch" type="xml">
            <form string="Dashboard Preference">
                <sheet>
                    <group>
                        <group>
                            <field name="user_id"/>
                            <field name="dashboard_id"/>
                        </group>
                        <group>
                            <field name="theme"/>
                            <field name="refresh_interval"/>
                        </group>
                    </group>

                    <group>
                        <group>
                            <field name="date_range"/>
                        </group>
                        <group>
                            <field name="visible_widgets"/>
                        </group>
                    </group>

                    <group string="Layout Customization">
                        <field name="widget_positions" widget="ace" options="{'mode': 'json'}"/>
                    </group>

                    <group string="Custom Filters">
                        <field name="custom_filters" widget="ace" options="{'mode': 'json'}"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Preference Action -->
    <record id="action_dashboard_preference" model="ir.actions.act_window">
        <field name="name">Dashboard Preferences</field>
        <field name="res_model">university.dashboard.preference</field>
        <field name="view_mode">list,form</field>
    </record>


    <!-- ============================================
         DASHBOARD ANALYTICS VIEWS
         ============================================ -->

    <!-- Analytics List View -->
    <record id="view_dashboard_analytics_list" model="ir.ui.view">
        <field name="name">university.dashboard.analytics.list</field>
        <field name="model">university.dashboard.analytics</field>
        <field name="arch" type="xml">
            <list string="Dashboard Analytics">
                <field name="name"/>
                <field name="code"/>
                <field name="category"/>
                <field name="chart_type"/>
                <field name="is_public" widget="boolean_toggle"/>
                <field name="active" widget="boolean_toggle"/>
            </list>
        </field>
    </record>

    <!-- Analytics Form View -->
    <record id="view_dashboard_analytics_form" model="ir.ui.view">
        <field name="name">university.dashboard.analytics.form</field>
        <field name="model">university.dashboard.analytics</field>
        <field name="arch" type="xml">
            <form string="Dashboard Analytics">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="category"/>
                        </group>
                        <group>
                            <field name="chart_type"/>
                            <field name="is_public"/>
                            <field name="active"/>
                        </group>
                    </group>

                    <group string="Description">
                        <field name="description" nolabel="1"/>
                    </group>

                    <notebook>
                        <page string="Data Configuration" name="data">
                            <group>
                                <field name="model_name"/>
                                <field name="query" widget="ace" options="{'mode': 'sql'}"/>
                            </group>
                        </page>

                        <page string="Access Control" name="access">
                            <field name="group_ids" widget="many2many_tags"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Analytics Action -->
    <record id="action_dashboard_analytics" model="ir.actions.act_window">
        <field name="name">Dashboard Analytics</field>
        <field name="res_model">university.dashboard.analytics</field>
        <field name="view_mode">list,form</field>
        <field name="context">{'search_default_active': 1}</field>
    </record>


    <!-- ============================================
         DASHBOARD SNAPSHOT VIEWS
         ============================================ -->

    <!-- Snapshot List View -->
    <record id="view_dashboard_snapshot_list" model="ir.ui.view">
        <field name="name">university.dashboard.snapshot.list</field>
        <field name="model">university.dashboard.snapshot</field>
        <field name="arch" type="xml">
            <list string="Dashboard Snapshots">
                <field name="name"/>
                <field name="dashboard_id"/>
                <field name="snapshot_date" widget="datetime"/>
                <field name="created_by"/>
            </list>
        </field>
    </record>

    <!-- Snapshot Form View -->
    <record id="view_dashboard_snapshot_form" model="ir.ui.view">
        <field name="name">university.dashboard.snapshot.form</field>
        <field name="model">university.dashboard.snapshot</field>
        <field name="arch" type="xml">
            <form string="Dashboard Snapshot">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="dashboard_id"/>
                        </group>
                        <group>
                            <field name="snapshot_date"/>
                            <field name="created_by"/>
                        </group>
                    </group>

                    <group string="Notes">
                        <field name="notes" nolabel="1"/>
                    </group>

                    <notebook>
                        <page string="KPI Data" name="kpi_data">
                            <field name="kpi_data" widget="ace" options="{'mode': 'json'}"/>
                        </page>

                        <page string="Analytics Data" name="analytics_data">
                            <field name="analytics_data" widget="ace" options="{'mode': 'json'}"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Snapshot Action -->
    <record id="action_dashboard_snapshot" model="ir.actions.act_window">
        <field name="name">Dashboard Snapshots</field>
        <field name="res_model">university.dashboard.snapshot</field>
        <field name="view_mode">list,form</field>
    </record>


    <!-- ============================================
         DASHBOARD ALERT VIEWS
         ============================================ -->

    <!-- Alert List View -->
    <record id="view_dashboard_alert_list" model="ir.ui.view">
        <field name="name">university.dashboard.alert.list</field>
        <field name="model">university.dashboard.alert</field>
        <field name="arch" type="xml">
            <list string="Dashboard Alerts" decoration-danger="alert_type=='error'"
                  decoration-warning="alert_type=='warning'" decoration-info="alert_type=='info'">
                <field name="name"/>
                <field name="alert_type"/>
                <field name="state"/>
                <field name="kpi_id"/>
                <field name="create_date" widget="datetime"/>
            </list>
        </field>
    </record>

    <!-- Alert Form View -->
    <record id="view_dashboard_alert_form" model="ir.ui.view">
        <field name="name">university.dashboard.alert.form</field>
        <field name="model">university.dashboard.alert</field>
        <field name="arch" type="xml">
            <form string="Dashboard Alert">
                <header>
                    <button name="action_acknowledge" type="object" string="Acknowledge"
                            class="btn-primary" invisible="state != 'new'"/>
                    <button name="action_resolve" type="object" string="Resolve"
                            class="btn-success" invisible="state != 'acknowledged'"/>
                    <button name="action_dismiss" type="object" string="Dismiss"
                            class="btn-secondary"/>
                    <field name="state" widget="statusbar"
                           statusbar_visible="new,acknowledged,resolved"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object"
                                class="oe_stat_button" icon="fa-archive">
                            <field name="active" widget="boolean_button"
                                   options='{"terminology": "archive"}'/>
                        </button>
                    </div>

                    <group>
                        <group>
                            <field name="name"/>
                            <field name="alert_type"/>
                            <field name="kpi_id"/>
                        </group>
                        <group>
                            <field name="threshold_breached" widget="boolean_toggle"/>
                            <field name="expiry_date"/>
                            <field name="is_expired" readonly="1"/>
                        </group>
                    </group>

                    <group string="Alert Message">
                        <field name="message" nolabel="1"/>
                    </group>

                    <group>
                        <group string="Action">
                            <field name="action_url"/>
                            <field name="action_label"/>
                        </group>
                        <group string="Status">
                            <field name="acknowledged_by" readonly="1"/>
                            <field name="acknowledged_date" readonly="1"/>
                            <field name="resolved_by" readonly="1"/>
                            <field name="resolved_date" readonly="1"/>
                        </group>
                    </group>

                    <notebook>
                        <page string="Recipients" name="recipients">
                            <group>
                                <field name="user_ids" widget="many2many_tags"/>
                                <field name="group_ids" widget="many2many_tags"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>

    <!-- Alert Kanban View -->
    <record id="view_dashboard_alert_kanban" model="ir.ui.view">
        <field name="name">university.dashboard.alert.kanban</field>
        <field name="model">university.dashboard.alert</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile" default_group_by="state">
                <field name="name"/>
                <field name="alert_type"/>
                <field name="state"/>
                <field name="message"/>
                <templates>
                    <t t-name="kanban-box">
                        <div class="oe_kanban_global_click">
                            <div class="o_kanban_record_top">
                                <div class="o_kanban_record_headings">
                                    <strong class="o_kanban_record_title">
                                        <field name="name"/>
                                    </strong>
                                </div>
                                <span t-attf-class="badge badge-pill badge-{{record.alert_type.raw_value}}">
                                    <field name="alert_type"/>
                                </span>
                            </div>
                            <div class="o_kanban_record_body">
                                <field name="message"/>
                            </div>
                            <div class="o_kanban_record_bottom">
                                <div class="oe_kanban_bottom_left">
                                    <field name="create_date" widget="date"/>
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <!-- Alert Search View -->
    <record id="view_dashboard_alert_search" model="ir.ui.view">
        <field name="name">university.dashboard.alert.search</field>
        <field name="model">university.dashboard.alert</field>
        <field name="arch" type="xml">
            <search string="Search Alerts">
                <field name="name"/>
                <field name="alert_type"/>
                <field name="state"/>
                <separator/>
                <filter string="New" name="new" domain="[('state', '=', 'new')]"/>
                <filter string="Acknowledged" name="acknowledged" domain="[('state', '=', 'acknowledged')]"/>
                <filter string="Resolved" name="resolved" domain="[('state', '=', 'resolved')]"/>
                <filter string="Active" name="active" domain="[('active', '=', True)]"/>
                <group expand="0" string="Group By">
                    <filter string="Alert Type" name="group_type" context="{'group_by': 'alert_type'}"/>
                    <filter string="State" name="group_state" context="{'group_by': 'state'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Alert Action -->
    <record id="action_dashboard_alert" model="ir.actions.act_window">
        <field name="name">Dashboard Alerts</field>
        <field name="res_model">university.dashboard.alert</field>
        <field name="view_mode">kanban,list,form</field>
        <field name="context">{'search_default_new': 1}</field>
    </record>


